{
    "$schema": "https://turborepo.org/schema.json",
    "pipeline": {
        "build": {
            "dependsOn": [
                "^build"
            ],
            "outputs": [
                ".svelte-kit/**"
            ]
        },
        "lint": {
            "outputs": []
        },
        "dev": {
            "cache": false
        },
        "package": {
            "outputs": [
                "package"
            ]
        }
    }
}